function formatTime(time) {
  var prefixPower = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : -3;
  var padLeftLength = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 0;
  var prefixes = ['n', 'Î¼', 'm', ''];
  var prefixIndex = Math.max(0, Math.min(Math.trunc(prefixPower / 3) + prefixes.length - 1, prefixes.length - 1));
  return String(time).padStart(padLeftLength) + " " + prefixes[prefixIndex] + "s";
}
